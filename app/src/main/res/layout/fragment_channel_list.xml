<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="view_model"
            type="com.example.rssreader.ui.channel.list.ChannelViewModel" />
    </data>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/channel_list"
        android:name="com.example.rssreader.ui.channel.dialog.list.ChannelFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        app:layoutManager="LinearLayoutManager"
        tools:context=".ui.channel.list.ChannelFragment"
        tools:listitem="@layout/fragment_channel_item" />
</layout>